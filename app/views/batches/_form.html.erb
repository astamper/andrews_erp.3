<%= simple_form_for(@batch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :manufacture_date %>
    <%= f.input :quantity %>
    <%= f.input :status %>
    <%= f.association :stocktype %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
